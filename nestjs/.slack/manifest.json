{
  "display_information": {
    "name": "Flicket AI2"
  },
  "features": {
    "app_home": {
      "home_tab_enabled": false,
      "messages_tab_enabled": true,
      "messages_tab_read_only_enabled": false
    },
    "bot_user": {
      "display_name": "FlicKAI2",
      "always_online": false
    },
    "assistant_view": {
      "assistant_description": "An Assistant built just for flicket"
    }
  },
  "oauth_config": {
    "scopes": {
      "bot": [
        "assistant:write",
        "channels:join",
        "im:history",
        "channels:history",
        "groups:history",
        "chat:write",
        "app_mentions:read",
        "users.profile:read",
        "files:read"
      ]
    }
  },
  "settings": {
    "event_subscriptions": {
      "bot_events": [
        "assistant_thread_context_changed",
        "assistant_thread_started",
        "message.im",
        "app_mention"
      ]
    },
    "interactivity": {
      "is_enabled": true
    },
    "org_deploy_enabled": true,
    "socket_mode_enabled": true,
    "token_rotation_enabled": false
  }
}
